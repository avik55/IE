/*header search bar css*/
header{position: relative; }
#header.fixed {transition: 0.6s all; position: fixed; top: 0; left: 0; width: 100%; z-index: 999; }
.collapse.navbar-collapse.navbar-right {position: relative;left: -5px;}
header .search {padding: 0px; margin: 0px; position: absolute; right: -18px; top:0px; z-index: 0; }
header .search .icon-srch {font-size: 16px;margin-top: 3px;}
header.fixed .search{top:4px;}
header.fixed .navbar-brand>img {top: 3px;position: relative;}
header .search.active {z-index: 1; } header .search button,
header .search input {padding: 8px 12px; border-radius: 0px; border-width: 0px; color: rgb(119, 119, 119); border-color: rgb(231, 231, 231); box-shadow: none; outline: none; }
header .search input {padding: 16px 12px; font-size: 14pt; }
header .search button[type="reset"] {display: none; }


@media (min-width: 768px) {
#header .navbar .container {padding: 0px 7px;}
.detail-page header#header,#header.fixed{background: #1e2633;padding: 5px 0px;}
header .search button{padding: 10px 12px;background: none;}
body header .search button:hover,
body header .search button:focus,
body header .search button:active{
outline: none !important;
background-color: transparent !important;
color:#e8e8e8;
}
header .search.active .btn.icon-srch{
display: none;
}
header .search input {padding: 20px 12px; font-size: 20px; opacity: 0; display: none; background: white; } 
header .search.active {background: #ccc;border-radius: 50px;}
header .search .btn.crs-srch {top: 2px;}
header .search.active input{border-radius: 50px;}
header .search.active button,
header .search.active input {
display: table-cell;
opacity: 1;
}
header .search.active input {
width: 100%;
}
}
/*header search bar css ends*/
