                    <div id="contactdialog" class="modal fade" role="dialog">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header" style="height:60px;">
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                    <div data-ng-show="ShowCntFrm" style="text-align: center;">
                                        <h3 style="color:white;font-size:20px;margin:0;">Contact Us</h3>
                                    </div>
                                    <div data-ng-hide="ShowCntFrm" style="text-align: center;">
                                        <h3 style="color:white;font-size:20px;margin:0;">Success</h3>
                                    </div>
                                </div>
                                <div class="modal-body">
                                    <div class="form log-reg">
                                        <div id="logindiv">
                                            <div data-ng-show="ShowCntFrm">
                                                <form name="contactform">
                                                    <fieldset>
                                                        <div class="field-wrap">
                                                            <label>
                                                                Name
                                                            </label>
                                                            <input class="form-control" type="text" data-ng-model="EntnMessage.Name" name="EntnMessage.Name" required>
                                                        </div>

                                                        <div class="field-wrap">
                                                            <label>
                                                                Email ID
                                                            </label>
                                                            <input class="form-control" type="email" data-ng-model="EntnMessage.EmailID" name="EntnMessage.EmailID" required>
                                                        </div>

                                                        <div class="field-wrap">
                                                            <label>
                                                                Contact No.
                                                            </label>
                                                            <input class="form-control" type="text" data-ng-model="EntnMessage.ContactNo" name="EntnMessage.ContactNo" required>
                                                        </div>

                                                        <div class="field-wrap">
                                                            <label>
                                                                Message
                                                            </label>
                                                            <textarea class="form-control" data-ng-model="EntnMessage.Message" placeholder="Max 100 Characters" rows="3" id="Message" required></textarea>
                                                        </div>
                                                        <p class="text-info">{{cntfrmMessage}}</p>
                                                        <button class="btnPopUp" type="submit" data-ng-disabled="!contactform.$valid" data-ng-click="SendMessagetoEntrn(EntnMessage)">Submit</button>
                                                    </fieldset>
                                                </form>
                                            </div>
                                            <div data-ng-hide="ShowCntFrm">
                                                <p class="text-info">{{cntfrmMessage}}</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

