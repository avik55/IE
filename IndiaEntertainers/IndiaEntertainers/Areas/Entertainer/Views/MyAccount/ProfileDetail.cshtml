                    <div id="contactdialog" class="modal fade" role="dialog">
                        <div class="modal-dialog">

                            <!-- Modal content-->
                            <div class="modal-content">
                                <div class="modal-body">
                                    <div class="form log-reg">
                                        <div id="logindiv">
                                            <h1>Contact Us</h1>
                                            <div data-ng-show="ShowCntFrm">
                                                <ng-form name="contactform">
                                                    <fieldset>
                                                        <div class="field-wrap">
                                                            <label>
                                                                Name<span class="req">* </span>
                                                            </label>
                                                            <input class="form-control" type="text" data-ng-model="EntnMessage.Name" name="Name" ng-required="cntfrmrqured">
                                                        </div>

                                                        <div class="field-wrap">
                                                            <label>
                                                                Email ID<span class="req">*</span>
                                                            </label>
                                                            <input class="form-control" type="email" data-ng-model="EntnMessage.EmailID" name="EmailID" ng-required="cntfrmrqured">
                                                        </div>

                                                        <div class="field-wrap">
                                                            <label>
                                                                Contact No.<span class="req">*</span>
                                                            </label>
                                                            <input class="form-control" type="text" data-ng-model="EntnMessage.ContactNo" name="ContactNo" ng-required="cntfrmrqured">
                                                        </div>

                                                        <div class="field-wrap">
                                                            <label>
                                                                Message<span class="req">*</span>
                                                            </label>
                                                            <textarea class="form-control" data-ng-model="EntnMessage.Message" placeholder="Max 100 Characters" rows="3" id="Message" ng-required="cntfrmrqured"></textarea>
                                                        </div>
                                                        <p class="text-info">{{cntfrmMessage}}</p>
                                                        <button class="button button-block" id="login_frm" type="submit" data-ng-disabled="!contactform.$valid" data-ng-click="SendMessagetoEntrn(EntnMessage)" value="login" name="login">Submit</button>
                                                    </fieldset>
                                                </ng-form>
                                            </div>
                                            <div data-ng-hide="ShowCntFrm">
                                                <p class="text-info">{{cntfrmMessage}}</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div id="changecoverimage" class="modal fade" role="dialog">
                        <div class="modal-dialog">
                            <!-- Modal content-->
                            <div class="modal-content">
                                <div class="modal-body">
                                    <div class="form log-reg">
                                        <div id="logindiv">
                                            <h1>Change Cover Image</h1>
                                            <ng-form name="CoverImgChageFrm">
                                                <fieldset>
                                                    <div data-ng-show="ShowChangeCoverModel">
                                                        <div class="field-wrap">
                                                            <label>
                                                                Title<span class="req">* </span>
                                                            </label>
                                                            <input class="form-control" type="text" data-ng-model="Title" name="Title">
                                                        </div>

                                                        <div class="field-wrap">
                                                            <label>
                                                                Select Cover Image<span class="req">* </span>
                                                            </label>
                                                            <input name="myFile" upload-file="myFile" type="file" />
                                                        </div>
                                                        <p class="text-info">{{coverImgMessage}}</p>
                                                        <button class="button button-block" type="button" ng-click="ChangeCoverImage()">Upload</button>
                                                    </div>
                                                    <div data-ng-hide="ShowChangeCoverModel">
                                                        <p class="text-info">{{coverImgMessage}}</p>
                                                    </div>

                                                    @*<button class="button button-block" type="button" ng-click="ChangeCoverImage()">Submit</button>*@
                                                </fieldset>
                                            </ng-form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="changeProfileimage" class="modal fade" role="dialog">
                        <div class="modal-dialog">
                            <!-- Modal content-->
                            <div class="modal-content">
                                <div class="modal-body">
                                    <div class="form log-reg">
                                        <div id="logindiv">
                                            <h1>Change Profile Picture</h1>
                                            <ng-form name="PrfImgChageFrm">
                                                <fieldset>
                                                    <div data-ng-show="ShowChangeProfileModel">
                                                        <div class="field-wrap">
                                                            <label>
                                                                Title<span class="req">* </span>
                                                            </label>
                                                            <input class="form-control" type="text" data-ng-model="Title" name="Title">
                                                        </div>

                                                        <div class="field-wrap">
                                                            <label>
                                                                Select Profile Pic.<span class="req">* </span>
                                                            </label>
                                                            <input name="myprofilePic" upload-file="myprofilePic" type="file" />
                                                        </div>
                                                        <p class="text-info">{{coverPrfMessage}}</p>
                                                        <button class="button button-block" type="button" ng-click="ChangeProfileImage()">Upload</button>
                                                    </div>
                                                    <div data-ng-hide="ShowChangeProfileModel">
                                                        <p class="text-info">{{coverPrfMessage}}</p>
                                                    </div>

                                                    @*<button class="button button-block" type="button" ng-click="ChangeCoverImage()">Submit</button>*@
                                                </fieldset>
                                            </ng-form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="AddNewImagee" class="modal fade" role="dialog">
                        <div class="modal-dialog">
                            <!-- Modal content-->
                            <div class="modal-content">
                                <div class="modal-body">
                                    <div class="form log-reg">
                                        <div id="logindiv">
                                            <h1>Add New Photo</h1>
                                            <ng-form name="PrfImgChageFrm">
                                                <fieldset>
                                                    <div data-ng-show="ShowAddphtoProfileModel">
                                                        <div class="field-wrap">
                                                            <label>
                                                                Title<span class="req">* </span>
                                                            </label>
                                                            <input class="form-control" type="text" data-ng-model="Title" name="Title">
                                                        </div>

                                                        <div class="field-wrap">
                                                            <label>
                                                                Select Profile Pic.<span class="req">* </span>
                                                            </label>
                                                            <input name="MyNewPhoto" upload-file="MyNewPhoto" type="file" />
                                                        </div>
                                                        <p class="text-info">{{NewPhotoMessage}}</p>
                                                        <button class="button button-block" type="button" ng-click="AddNewPhoto()">Upload</button>
                                                    </div>
                                                    <div data-ng-hide="ShowAddphtoProfileModel">
                                                        <p class="text-info">{{NewPhotoMessage}}</p>
                                                    </div>

                                                    @*<button class="button button-block" type="button" ng-click="ChangeCoverImage()">Submit</button>*@
                                                </fieldset>
                                            </ng-form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="AddNewVideoe" class="modal fade" role="dialog">
                        <div class="modal-dialog">
                            <!-- Modal content-->
                            <div class="modal-content">
                                <div class="modal-body">
                                    <div class="form log-reg">
                                        <div id="logindiv">
                                            <h1>Add New Video</h1>
                                            <ng-form name="PrfImgChageFrm">
                                                <fieldset>
                                                    <div data-ng-show="ShowAddvdoProfileModel">
                                                        <div class="field-wrap">
                                                            <label>
                                                                Title<span class="req"> </span>
                                                            </label>
                                                            <input class="form-control" type="text" data-ng-model="NewWid.Title" name="Title">
                                                        </div>

                                                        <div class="field-wrap">
                                                            <label>
                                                                Video Link<span class="req">* </span>
                                                            </label>
                                                            <input data-ng-model="NewWid.Videolink" type="url" />
                                                        </div>
                                                        <p class="text-info">{{AdVdMessage}}</p>
                                                        <button class="button button-block" type="button" ng-click="AddVideo(NewWid)">Upload</button>
                                                    </div>
                                                    <div data-ng-hide="ShowAddvdoProfileModel">
                                                        <p class="text-info">{{AdVdMessage}}</p>
                                                    </div>

                                                    @*<button class="button button-block" type="button" ng-click="ChangeCoverImage()">Submit</button>*@
                                                </fieldset>
                                            </ng-form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="UpdateProfileModal" class="modal fade" role="dialog">
                        <div class="modal-dialog">
                            <!-- Modal content-->
                            <div class="modal-content">
                                <div class="modal-body">
                                    <div class="form log-reg">
                                        <div id="logindiv">
                                            <h1>Update Profile</h1>
                                            <ng-form name="PrfImgChageFrm">
                                                <fieldset>
                                                    <div data-ng-show="ShowUpdateProfileModel">
                                                        <div class="field-wrap">
                                                            <label>
                                                                Profile<span class="req"> </span>
                                                            </label>
                                                            <textarea class="form-control" data-ng-model="profile" maxlength="150" name="profile" placeholder="Max 150 Characters" rows="6" id="comment"></textarea>
                                                        </div>
                                                        <p class="text-info">{{UpProfileMessage}}</p>
                                                        <button class="button button-block" type="button" ng-click="UpdateProfile(profile)">Update</button>
                                                    </div>
                                                    <div data-ng-hide="ShowUpdateProfileModel">
                                                        <p class="text-info">{{UpProfileMessage}}</p>
                                                    </div>

                                                    @*<button class="button button-block" type="button" ng-click="ChangeCoverImage()">Submit</button>*@
                                                </fieldset>
                                            </ng-form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="UpdateProDetails" class="modal fade" role="dialog">
                        <div class="modal-dialog">
                            <!-- Modal content-->
                            <div class="modal-content">
                                <div class="modal-body">
                                    <div class="form log-reg">
                                        <div id="logindiv">
                                            <h1>Update Profile</h1>
                                            <ng-form name="PrfImgChageFrm">
                                                <fieldset>
                                                    <div data-ng-show="ShowUpdateProfileModel">
                                                        <div class="field-wrap">
                                                            <label>
                                                                Gender<span class="req"> </span>
                                                            </label>
